    //    function updateTokenMetadata(uint256 _tokenId, string _uri) public onlyTokenOwner(_tokenId) onlyTokenCreator(_tokenId) {
    //        _setTokenURI(_tokenId, _uri);
    //        emit TokenURIUpdated(_tokenId, _uri);
    //    }

    /**
